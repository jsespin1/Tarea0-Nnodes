<!DOCTYPE html>
<html>
  <head>
    <title>Tarea0Je</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
  	<% if flash[:alert] %>
	    <p class="flash #{key} alert"><%= flash[:alert] %></p>
	  <% end %>
	  <% if flash[:notice] %>
	    <p class="flash #{key} notice"><%= flash[:notice] %></p>
	<% end %>
    <%= yield %>
  </body>
</html>
